onStart => {
	//for {
		//play chomp, true
	//}
	play chomp, true, true
}

onClick => {
	quote "m"
	step 100
	step -100
	turn -360
	quote ""
	broadcast "quote"
}

onKey KeyQ, => {
	quote "m", "monkey", 1
}
onKey KeyW, => {
	play clap
}
onKey KeyH, => {
	stopSound chomp
}

onKey KeyA, => {
	pauseSound clap
}

onKey KeyB, => {
	resumeSound clap
}
