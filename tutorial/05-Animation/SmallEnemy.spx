var life int
onStart => {
	for {
		wait 0.1
		clone
	}
}

onCloned => {
	life = 3
	setXYpos rand(-131, 131), 237
	show
	for {
		waitNextFrame
		changeYpos -200 * deltaTime
		if touching(EdgeBottom) {
			destroy
		}
	}
}

onTouchStart "Bullet", => {
	if life > 0 {
        life --
        if life <= 0 {
            die
        }
    }
}